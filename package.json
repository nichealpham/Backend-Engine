{
	"name": "microserver-less",
	"version": "1.1.0",
	"description": "Backend design pattern for Google Cloud App Engine, AWS Lambda and Docker",
	"scripts": {
		"dev": "./node_modules/.bin/ts-node ./src/server.ts",
		"eslint": "./node_modules/.bin/eslint --ext .ts --ext .js ./src",
		"build": "npm run eslint && ./node_modules/.bin/tsc && ./node_modules/.bin/grunt copy",
		"test": "npm run build && node dest/system/UnitTest.js",
		"staging": "npm run build && node dest/server.js",
		"docker": "npm i typescript ts-node @types/node && ./node_modules/.bin/ts-node src/system/DockerPipeline.ts",
		"deploy-gcp": "npm run build && cp package.json dest/ && cd dest && gcloud app deploy --project testengine18238",
		"deploy-sls": "npm run build && cp package.json dest/ && cd dest && npm install --only=production && sls deploy",
		"start": "NODE_ENV=Production node dest/server.js",
		"commit": "git push origin dev && git checkout master && git merge dev && git push origin master && git checkout dev"
	},
	"dependencies": {
		"@google-cloud/storage": "^1.7.0",
		"body-parser": "^1.18.3",
		"cors": "^2.8.4",
		"express": "^4.16.3",
		"firebase-admin": "^6.0.0",
		"mongoose": "^5.2.14",
		"multer": "^1.3.1",
		"request-promise": "^4.2.2",
		"serverless-http": "^1.6.0",
		"rimraf": "^2.6.2"
	},
	"devDependencies": {
		"@types/body-parser": "^1.17.0",
		"@types/express": "^4.16.0",
		"@types/mongoose": "^5.2.12",
		"@types/multer": "^1.3.7",
		"@types/node": "^10.9.4",
		"@types/request-promise": "^4.1.42",
		"@types/rimraf": "^2.0.2",
		"eslint": "^5.8.0",
		"eslint-config-google": "^0.11.0",
		"grunt": "^1.0.3",
		"grunt-contrib-copy": "^1.0.0",
		"ts-node": "^6.0.0",
		"typescript": "^3.1.6",
		"typescript-eslint-parser": "^20.1.1"
	},
	"keywords": [
		"cloud-storage",
		"firestore",
		"nodejs",
		"docker",
		"ts"
	],
	"author": "Nicheal Pham",
	"license": "MIT"
}